---
apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  name: {{ project_name }}
  namespace: {{ namespace }}
spec:
  rules:
    - host: {{ ingress_host }}
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              serviceName: {{ project_name }}
              servicePort: {{ service_port }}
            
